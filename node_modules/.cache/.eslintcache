f snacks or a list of drinks */}\n        <Route path=\"/:type\" >\n        <Menu foods={foods} title=\"Menu\" />\n        </Route>\n\n        \n        {/* 4- redirect to \"/\" */}\n        <Route>\n        <p>Hmmm. I can't seem to find what you want.</p>\n        </Route>\n\n      </Switch>   \n);                   \n}\n\nexport default Routes;\n","/Users/abbycai/Downloads/snack-or-booze/src/NavBar.js",[],"/Users/abbycai/Downloads/snack-or-booze/src/Api.js",[],"/Users/abbycai/Downloads/snack-or-booze/src/Home.js",[],"/Users/abbycai/Downloads/snack-or-booze/src/FoodMenu.js",[],"/Users/abbycai/Downloads/snack-or-booze/src/FoodItem.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":2,"column":31,"nodeType":"57","messageId":"58","endLine":2,"endColumn":36},{"ruleId":"55","severity":1,"message":"59","line":6,"column":8,"nodeType":"57","messageId":"58","endLine":6,"endColumn":19},{"ruleId":"55","severity":1,"message":"60","line":12,"column":10,"nodeType":"57","messageId":"58","endLine":12,"endColumn":16},{"ruleId":"55","severity":1,"message":"61","line":1,"column":27,"nodeType":"57","messageId":"58","endLine":1,"endColumn":36},"no-native-reassign",["62"],"no-negated-in-lhs",["63"],"no-unused-vars","'Route' is defined but never used.","Identifier","unusedVar","'NewFoodForm' is defined but never used.","'snacks' is assigned a value but never used.","'useEffect' is defined but never used.","no-global-assign","no-unsafe-negation"]